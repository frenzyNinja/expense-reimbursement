    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_1296763_expense.GetClaimDetails</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>Return claim details</description>
        <name>GetClaimDetails</name>
        <script><![CDATA[var GetClaimDetails = Class.create();
GetClaimDetails.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    getStatus: function() {
        var expenseID = this.getParameter('sysparm_expense_id');
		
		var gr = new GlideRecord('x_1296763_expense_reimbursement_expense');
		gr.addQuery('number', expenseID);
		gr.query();
	
		if (gr.next()) {
			var claim = gr.claim;
			return claim.status;
		}
		
		return '';
    },
    type: 'GetClaimDetails'
});]]></script>
